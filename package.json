{
  "name": "sgcenario",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev | pino-pretty",
    "build": "vite build",
    "preview": "vite preview",
    "sync": "svelte-kit sync",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "validate": "pnpm format && pnpm check && pnpm lint",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:generate-empty": "drizzle-kit generate:sqlite --custom",
    "db:pull": "drizzle-kit introspect:sqlite",
    "db:push": "drizzle-kit push:sqlite",
    "db:drop": "drizzle-kit drop",
    "db:check": "drizzle-kit check:sqlite",
    "db:studio": "drizzle-kit studio --verbose"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@libsql/client": "^0.3.5",
    "@lucia-auth/adapter-sqlite": "^2.0.0",
    "@lucia-auth/oauth": "^3.3.1",
    "@melt-ui/svelte": "^0.50.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.25.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.8.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.16",
    "better-sqlite3": "^8.7.0",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.13",
    "drizzle-orm": "^0.28.6",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "hangul-js": "^0.2.6",
    "lucia": "^2.7.1",
    "lucide-svelte": "^0.279.0",
    "nanoid": "^5.0.1",
    "overlayscrollbars": "^2.3.2",
    "overlayscrollbars-svelte": "^0.5.1",
    "pino": "^8.16.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.5",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-floating-ui": "^1.5.3",
    "tailwind-merge": "^1.14.0",
    "tailwind-variants": "^0.1.14",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  },
  "type": "module"
}
